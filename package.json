{
    "name": "nhentai-discord-bot",
    "version": "5.0.0",
    "description": "A Discord bot specializes in nhentai (but it also does other things)",
    "type": "module",
    "main": "/build/src/index.js",
    "author": "t41y0u <vuthaiduong0607@gmail.com>",
    "license": "AGPL-3.0",
    "dependencies": {
        "@discordjs/rest": "^0.1.0-canary.0",
        "axios": "^0.21.1",
        "discord.js": "^13.1.0",
        "dotenv": "^10.0.0",
        "i18next": "^21.3.1",
        "i18next-fs-backend": "^1.1.1",
        "moment": "^2.29.1",
        "mongoose": "^6.0.10",
        "pino": "^6.13.1",
        "pino-pretty": "^6.0.0",
        "readdirp": "^3.6.0",
        "reflect-metadata": "^0.1.13",
        "tsyringe": "^4.6.0"
    },
    "devDependencies": {
        "@types/i18next-fs-backend": "^1.1.2",
        "@types/node": "^15.12.4",
        "@types/pino": "^6.3.11",
        "@types/ws": "^7.4.5",
        "discord-api-types": "^0.22.0",
        "eslint": "^7.29.0",
        "rimraf": "^3.0.2",
        "typescript": "^4.3.4"
    },
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "clean": "rimraf dist",
        "prebuild": "npm run clean",
        "build": "tsc",
        "postbuild": "npm run copy-locales",
        "copy-locales": "cpy locales/**/*.* dist/ --parents",
        "lint": "eslint src --ext .ts",
        "lint:fix": "eslint src --ext .ts --fix",
        "start": "node --enable-source-maps --es-module-specifier-resolution=node build/src/index.js",
        "dev": "yarn prebuild && yarn build && yarn start",
        "deploy:commands": "yarn build && node --enable-source-maps --es-module-specifier-resolution=node build/src/deploy.js"
    },
    "engines": {
        "node": ">=16.6.0"
    }
}
